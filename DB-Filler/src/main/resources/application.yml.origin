logging:
  level:
    root: error
    com.shadowflex.DB-Filler: trace
  file:
    name: application.log

spring:
  main:
    banner-mode: off
  datasource:
    url: # + ?stringtype=unspecified
    username:
    password:
    driver-class-name: org.postgresql.Driver
  jpa:
    properties:
      hibernate:
        dialect: org.hibernate.dialect.PostgreSQLDialect
  mvc:
    hiddenmethod:
      filter:
        enabled: true

server:
  port: 80


